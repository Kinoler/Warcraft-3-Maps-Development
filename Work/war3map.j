function I1L1L111 takes nothing returns nothing
local itemtype H4
local integer H5=0
local integer H3=0
local integer H2
set H2=1
loop
if bj_stockAllowedPermanent[H2] then
set H3=H3+1
if GetRandomInt(1,H3)==1 then
set H4=ITEM_TYPE_PERMANENT
set H5=H2
endif
endif
if bj_stockAllowedCharged[H2] then
set H3=H3+1
if GetRandomInt(1,H3)==1 then
set H4=ITEM_TYPE_CHARGED
set H5=H2
endif
endif
if bj_stockAllowedArtifact[H2] then
set H3=H3+1
if GetRandomInt(1,H3)==1 then
set H4=ITEM_TYPE_ARTIFACT
set H5=H2
endif
