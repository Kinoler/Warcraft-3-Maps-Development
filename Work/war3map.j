function I1ILIL11 takes real an returns nothing
local real aq
local real ap=TimerGetElapsed(ao)
if ap<=0 then
set ao=CreateTimer()
call TimerStart(ao,$F4240,false,null)
endif
if an>0 then
loop
set aq=an-TimerGetElapsed(ao)+ap
exitwhen aq<=0
if aq>bj_POLLED_WAIT_SKIP_THRESHOLD then
call TriggerSleepAction(.1*aq)
else
call TriggerSleepAction(bj_POLLED_WAIT_INTERVAL)
endif
endloop
endif
endfunction
