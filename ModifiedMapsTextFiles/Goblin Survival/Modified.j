set yvel=vel*Sin(GetRandomReal(0,360.00)*bj_DEGTORAD)
call SetUnitLifeBJ(gu,(GetUnitStateSwap(UNIT_STATE_LIFE,gu)+R))
call SetTextTagText(TXT,I2S(R2I(R)),12*0.023/10.)
call SetTextTagPos(TXT,GetUnitX(gu),GetUnitY(gu),60)
call SetTextTagColor(TXT,PercentTo255(0),PercentTo255(100),PercentTo255(0),PercentTo255(100.))
call SetTextTagPermanent(TXT,true)
call SetTextTagVelocity(TXT,xvel,yvel)
call SetTextTagVisibility(TXT,true)
call SetTextTagLifespan(TXT,1.3)
call SetTextTagFadepoint(TXT,.3)
call SetTextTagPermanent(TXT,false)
else
call DestroyTextTag(TXT)
endif
endif
if GetUnitManaPercent(MEGA)<=3.00 then
call UnitRemoveBuffBJ(0x42303433,MEGA)
endif
set TXT=null
call GroupRemoveUnit(udg_TempGroup,gu)
set gu=null
endloop
set MEGA=null
endfunction
