call SetTextTagVelocity(TXT,0.05*Cos(GetRandomReal(0,360.00)*bj_DEGTORAD),0.05*Sin(GetRandomReal(0,360.00)*bj_DEGTORAD))
call SetTextTagVisibility(TXT,true)
call SetTextTagLifespan(TXT,1.3)
call SetTextTagFadepoint(TXT,.3)
call SetTextTagPermanent(TXT,false)
set TXT=null
endfunction
