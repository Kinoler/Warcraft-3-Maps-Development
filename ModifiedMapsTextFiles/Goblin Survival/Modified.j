function MakeNumberTag takes unit u,real i,integer r,integer g,integer b returns nothing
local texttag TXT=CreateTextTag()
call SetTextTagColor(TXT,r,g,b,255)
call SetTextTagText(TXT,I2S(R2I(i)),0.0275)
call SetTextTagPos(TXT,GetUnitX(u),GetUnitY(u),60)
