local real d
if u!=udg_Boss and u!=udg_Boss_KV then
set i=GetHandleId(u)
set d=LoadReal(udg_H,i,200)
if d==0 then
call PauseUnit(u,true)
call UnitAddAbility(u,0x41305942)
set d=LoadInteger(udg_H,0,200)
call SaveInteger(udg_H,0,200,R2I(d)+1)
