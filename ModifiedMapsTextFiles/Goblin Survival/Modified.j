call TriggerRegisterAnyUnitEventBJ(gg_trg_Mehagoblin,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Mehagoblin,Condition(function Trig_Mehagoblin_Conditions))
call TriggerAddAction(gg_trg_Mehagoblin,function Trig_Mehagoblin_Actions)
endfunction
function Trig_Death_soul_steal_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()==0x41304944))then
return false
endif
return true
endfunction
function Trig_Death_soul_steal_Func003Func001C takes nothing returns boolean
if(not(IsPlayerEnemy(GetOwningPlayer(GetEnumUnit()),GetOwningPlayer(GetTriggerUnit()))==true))then
return false
endif
return true
endfunction
function Trig_Death_soul_steal_Func003A takes nothing returns nothing
if(Trig_Death_soul_steal_Func003Func001C())then
call UnitDamageTargetBJ(GetTriggerUnit(),GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_MANA,GetTriggerUnit())/2.00),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_MAGIC)
set udg_soulsteal=(udg_soulsteal+1)
else
endif
endfunction
function Trig_Death_soul_steal_Actions takes nothing returns nothing
set udg_soulsteal=0
set bj_wantDestroyGroup=true
call ForGroupBJ(GetUnitsInRangeOfLocAll(800.00,GetUnitLoc(GetTriggerUnit())),function Trig_Death_soul_steal_Func003A)
call SetUnitManaBJ(GetTriggerUnit(),(GetUnitStateSwap(UNIT_STATE_MANA,GetTriggerUnit())+(I2R(udg_soulsteal)*100.00)))
endfunction
function Zloy_Custom_Table takes nothing returns nothing
set gg_trg_Death_soul_steal=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Death_soul_steal,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Death_soul_steal,Condition(function Trig_Death_soul_steal_Conditions))
call TriggerAddAction(gg_trg_Death_soul_steal,function Trig_Death_soul_steal_Actions)
endfunction
