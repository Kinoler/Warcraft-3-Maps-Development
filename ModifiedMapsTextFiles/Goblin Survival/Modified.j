local real X=GetUnitX(MEGA)
local real Y=GetUnitY(MEGA)
local texttag TXT
local real vel
local real xvel
local real yvel
call SetUnitManaPercentBJ(MEGA,GetUnitManaPercent(MEGA)-3)
call GroupEnumUnitsInRange(udg_TempGroup,X,Y,500.,null)
loop
set gu=FirstOfGroup(udg_TempGroup)
exitwhen gu==null
if MEGA!=gu and GetOwningPlayer(gu)!=Player(PLAYER_NEUTRAL_PASSIVE)then
if not IsPlayerEnemy(GetOwningPlayer(MEGA),GetOwningPlayer(gu))and not IsUnitType(gu,UNIT_TYPE_MAGIC_IMMUNE)and not IsUnitType(gu,UNIT_TYPE_STRUCTURE)then
set TXT=CreateTextTag()
